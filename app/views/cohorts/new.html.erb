<%= form_for :cohorts, url: cohorts_path do |f| %>
  <p>
    <%= f.label :Name %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :Active %>
    <%= f.check_box :active %>
  </p>
  <p>
    <%= f.label :Pitch_limit %>
    <%= f.number_field :pitch_limit %>
  </p>
  <p>
    <%= f.label :Pitch_start %>
    <%= f.date_field :pitch_start %>
  </p>
  <p>
    <%= f.label :Vote_limit %>
    <%= f.number_field :vote_limit %>
  </p>
  <p>
    <%= f.label :Vote_start %>
    <%= f.date_field :vote_start %>
  </p>
  <p>
    <%= f.label :Vote_end %>
    <%= f.date_field :vote_end %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>